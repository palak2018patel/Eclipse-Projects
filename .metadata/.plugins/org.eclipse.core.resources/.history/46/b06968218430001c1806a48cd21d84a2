package CollectionsFrameworkASSIGNMENTS;
import java.util.ArrayList;
import java.util.Collections;
import java.util.Iterator;
import java.util.List;

public class StudentMarks {		
		
			public class Student {				
				String name;
				char grade;
				int marks;	
			
			public Student(String name,char grade,int marks) {
				this.name=name;
				this.grade=grade;
				this.marks=marks;
			}
				public void main(String[] args) {	
				ArrayList<Object> Arr= new ArrayList<Object>();
				Student s1= new Student("lmn",'A',100);
				Student s2= new Student("xyz",'B',40);
				Student s3= new Student("abc",'C',75);
				Student s4= new Student("uvw",'D',50);		
				Student s5= new Student("pqr",'E',80);		
				ArrayList<Integer>list=new ArrayList<Integer>();
				list.add(s1.marks);
				list.add(s2.marks);
				list.add(s3.marks);
				list.add(s4.marks);
				list.add(s5.marks);
				Collections.sort( list,Collections.reverseOrder());		
				
				System.out.println("List after sorting: "+list);	
				
					
				}
				
				
			}

			

	}


